<p id="notice"><%= notice %></p>


<!-- change to div class=container overlay -->
<div class = "container overlay">
  <table>
    <h2>All Resources</h2>
    <thead>
      <tr>
        <th>Name</th>
        <th>Url</th>
        <th>Description</th>
        <th>Rank</th>
        <th>Comment_ids</th>
        <!-- <th colspan="3"></th> -->
      </tr>
    </thead>

    <tbody>
      <% @resources.each do |resource| %>
        <tr>
          <td align = "left"><%= resource.name %></td>
          <td align = "left"><%= link_to resource.url, resource.url%></td>
          <td align = "left"><%= resource.description %></td>
          <td align = "left"><%= resource.rank %></td>
          <td align = "left"><%= resource.comment_ids %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

<br>


